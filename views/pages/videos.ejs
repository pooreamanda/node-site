<%- contentFor('hero') %>

<section class="hero videos">
	<div class="hero-content">
		<h2>Videos</h2>
		<p>See me in action</p>
	</div>
</section>

<%- contentFor('body') %>

<!-- YouTube Video -->

<% if (videoLinks) { %>
	<% videoLinks.forEach((url) => { %>
	    <div class="video">
	        <div style="position:relative;height:0;padding-bottom:56.25%">
				<iframe src= "<%= url  %>" height="315" frameborder="0" style="position:absolute;width:100%;height:100%;left:0;padding-bottom:10px" allowfullscreen></iframe></div>
	    </div>
   	<% }) %>
<% } %>

